<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jjgo.education.education">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- MainActivity -->
        <activity
            android:name=".Main"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            android:screenOrientation="portrait"/>
        <!-- 4D center -->
        <activity
            android:name=".FourDCenter"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity android:name=".SealedCenter"
            android:screenOrientation="portrait"/>
        <activity android:name=".SafetyCenter"
            android:screenOrientation="portrait"/>
        <activity android:name=".SeatbeltCenter"
            android:screenOrientation="portrait"/>
        <activity android:name=".ExtinguisherCenter"
            android:screenOrientation="portrait"/>
        <activity android:name=".FallsCenter"
            android:screenOrientation="portrait"/>
        <activity android:name=".PreventFallCenter"
            android:screenOrientation="portrait"/>
        <activity android:name=".ManageHeavyCenter"
            android:screenOrientation="portrait"/>
        <activity android:name=".RopeCrankingCenter"
            android:screenOrientation="portrait"/>
        <activity android:name=".GasExplodeCenter"
            android:screenOrientation="portrait"/>
        <activity android:name=".ElectricityCenter"
            android:screenOrientation="portrait"/>
        <activity android:name=".CPRCenter"
            android:screenOrientation="portrait"/>
        <activity android:name=".AlcoNicoCenter"
            android:screenOrientation="portrait"/>
        <activity android:name=".HeavyLiftingCenter"
            android:screenOrientation="portrait"/>
    </application>

</manifest>